#: transactions/models.py:23
msgid "Updated"
msgstr "به‌روزرسانی"

#: transactions/models.py:24
msgid "The date and time when this object was last updated."
msgstr "تاریخ و زمان آخرین به‌روزرسانی این شیء."

#: transactions/models.py:36
msgid "UUID"
msgstr "شناسه"

#: transactions/models.py:37
msgid "A unique identifier for this object."
msgstr "یک شناسه یکتا برای این شیء."

#: transactions/models.py:49
msgid "Balance"
msgstr "موجودی"

#: transactions/models.py:50
msgid "The current balance of the wallet."
msgstr "موجودی فعلی کیف پول."

#: transactions/models.py:96
msgid "Wallet"
msgstr "کیف پول"

#: transactions/models.py:97
msgid "Wallets"
msgstr "کیف‌های پول"

#: transactions/models.py:104
#, python-format
msgid "The balance must always be positive. Got %(show_value)s."
msgstr "موجودی باید همیشه مثبت باشد. مقدار دریافت شده %(show_value)s."

#: transactions/models.py:112
msgid "Pending"
msgstr "در انتظار"

#: transactions/models.py:113
msgid "Success"
msgstr "موفقیت‌آمیز"

#: transactions/models.py:114
msgid "Failed"
msgstr "ناموفق"

#: transactions/models.py:120
msgid "Sender Wallet"
msgstr "فرستنده"

#: transactions/models.py:121
msgid "The wallet from which the transaction is made."
msgstr "کیف پولی که تراکنش از آن انجام شده است."

#: transactions/models.py:127
msgid "Receiver Wallet"
msgstr "گیرنده"

#: transactions/models.py:128
msgid "The wallet to which the transaction is made."
msgstr "کیف پولی که تراکنش به آن انجام شده است."

#: transactions/models.py:133 transactions/serializers.py:38
#: transactions/serializers.py:51
